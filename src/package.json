{
  "name": "nativescript-orientation-ssi",
  "version": "2.2.4",
  "description": "A NativeScript plugin to deal with Declarative UI and Screen Orientation. Since the original plugin requires a subscription now, this plugin is my attempt to maintain it to the point where it does not break.",
  "main": "orientation",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "6.0.0",
      "ios": "6.0.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "vue": "true",
      "core3": "true",
      "category": "Processing"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibnYusrat/nativescript-orientation"
  },
  "dependencies": {
    "nativescript-globalevents-free": "latest",
    "nativescript-dom-free": "latest"
  },
  "keywords": [
    "NativeScript",
    "orientation",
    "landscape",
    "ios",
    "android"
  ],
